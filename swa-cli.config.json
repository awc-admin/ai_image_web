{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "ai-image-web": {
      "appLocation": "app",
      "apiLocation": "api",
      "outputLocation": "app/build",
      "apiLanguage": "python",
      "apiVersion": "4.0",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "pip install -r requirements.txt",
      "run": "npm start",
      "appDevserverUrl": "http://localhost:3000"
    }
  }
}