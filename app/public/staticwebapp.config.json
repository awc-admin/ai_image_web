{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api-proxy/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "rewrite": "http://20.11.8.84:5000/{rest}",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/images/*.{png,jpg,gif}", "/css/*", "/*.{ico,png,jpg,gif}", "/*.js", "/*.css", "/mockAuth.js"]
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}